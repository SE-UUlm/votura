{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": ["DATABASE_URL"],
  "ui": "stream",
  "concurrency": "100%",
  "cacheDir": ".turbo/cache",
  "envMode": "strict",
  "tasks": {
    "lint": {
      "dependsOn": ["^lint"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false
    },
    "format": {
      "dependsOn": ["^format"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false
    },
    "check-format": {
      "dependsOn": ["^check-format"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "env": ["PORT", "TERMINAL_LOG_LEVEL"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false,
      "outputs": ["build/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": false,
      "interactive": false
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$"],
      "outputLogs": "full",
      "persistent": true,
      "interactive": false
    }
  }
}
